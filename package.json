{
  "version": "0.1.0",
  "private": true,
  "homepage": "https://LouayLH10.github.io/louayind",
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "npm --prefix client start",
    "start:server": "npm --prefix server start",
    "install:all": "npm --prefix client install && npm --prefix server install",
    "build": "npm --prefix client run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/build"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
